            0x00000004 | 0x00000008 | 0x00000020,
            0x00000004, 0x0000000A);

    SSIDisable(0x40008000);

    SSIConfigSetExpClk(0x40008000, SysCtlClockGet(), 0x00000000,
            0x00000000, 2000000, 8);

    GPIOPinTypeSSI(0x40004000, 0x00000008);
    GPIOPadConfigSet(0x40004000, 0x00000008,
            0x00000004, 0x0000000A);

    SSIEnable(0x40008000);
}

void Zone_Enable(int zone)
{
    int val;

    while(SSIBusy(0x40008000)) {}

    val = 0x01 << (zone - 1);
    SSIDataPut(0x40008000, val);
}

void Zone_Disable(void)
{
    while(SSIBusy(0x40008000)) {}

    SSIDataPut(0x40008000, ZONE_NONE);
}

int Zone_Current(void)
{
    return Current_Zone;
}
