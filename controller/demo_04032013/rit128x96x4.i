{
    
    
    
    if(!(*((volatile unsigned long *)(((unsigned long)(&g_ulSSIFlags) & 0xF0000000) | 0x02000000 | (((unsigned long)(&g_ulSSIFlags) & 0x000FFFFF) << 5) | ((0) << 2)))))
