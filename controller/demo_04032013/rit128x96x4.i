    if((*((volatile unsigned long *)(((unsigned long)(&g_ulSSIFlags) & 0xF0000000) | 0x02000000 | (((unsigned long)(&g_ulSSIFlags) & 0x000FFFFF) << 5) | ((1) << 2)))))
    {
        
        
        
        
        while(SSIBusy(0x40008000))
        {
        }

        
        
        
        GPIOPinWrite(0x40027000, 0x00000004, 0);
        (*((volatile unsigned long *)(((unsigned long)(&g_ulSSIFlags) & 0xF0000000) | 0x02000000 | (((unsigned long)(&g_ulSSIFlags) & 0x000FFFFF) << 5) | ((1) << 2)))) = 0;
    }

    
    
    
    while(ulCount != 0)
    {
        
        
        
        SSIDataPut(0x40008000, *pucBuffer++);

        
        
        
        ulCount--;
    }
}













